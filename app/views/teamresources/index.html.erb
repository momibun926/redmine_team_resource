<% html_title("Team resource monitoring") %>
<% content_for :header_tags do %>
  <%= stylesheet_link_tag "team_resource",   plugin: "redmine_team_resource" %>
<% end %>
<%= @sumiraize_month %>
<% before_user = "" %>
<% @sumiraize_month.keys.each do |k| %>
  <% current_user = User.find_by(id: get_user_id(k)) || "Not Assigned" %><br>
  <%= current_user if before_user != current_user  %><br>
  <%= Project.find_by(id: get_project_id(k)) %><br>
  <%= @sumiraize_month[k] %><br>
  <% before_user = current_user %>
<% end %>